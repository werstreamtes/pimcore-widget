{% set urlField = pimcore_input('url') %}
{% set Disc = pimcore_checkbox('Disc') %}
{% set TV = pimcore_checkbox('TV') %}
{% set Trailer = pimcore_checkbox('Trailer') %}

{% if editmode %}
    <div class="wse-widget">
        <img
            src="data:image/svg+xml;base64,"
            alt="WerStreamt.es Logo"/>
        <div class="wse-widget-inner">
            <label>URL zum Film / zur Serie</label>
            <div class="wse-input">
                {{ urlField | raw }}
            </div>
        </div>
    </div>
    <style>
        .wse-widget label {
            font-size: 11px;
            margin-bottom: 5px;
            display: block;
        }

        .wse-widget img {
            height: 45px;
            position: absolute;
            top: -15px;
            left: 5px;
        }

        .wse-widget .wse-input {
            background: #fff;
            padding: 3px 5px;
            border-radius: 2px;
            margin-top: 2px;
        }

        .wse-widget {
            border: 1px solid #fff;
            background: #DFAD05;
            color: #000;
            border-radius: 4px;
            box-shadow: 0 0 5px 0 #000;
        }

        .wse-widget .wse-widget-inner {
            border-bottom: 10px solid #302B25;
            padding: 25px 20px 15px;
            border-bottom-left-radius: 4px;
            border-bottom-right-radius: 4px;
        }
    </style>
{% else %}
    {% if not urlField.isEmpty() %}
        <div
            class="wse-widget {{ Disc != "1" ? "wse-no-disc" : "" }} {{ TV != "1" ? "wse-no-tv" : "" }} {{ Trailer != "1" ? "wse-no-trailer" : "" }}">
            <a rel="noopener" href="{{ urlField }}?tag={{ pimcore_website_config('wseWidgetTag', 'extern') }}"
               target="_blank">Verfügbarkeit auf WerStreamt.es prüfen</a>
            <script async src="https://www.werstreamt.es/_resources/themes/wse/js/widget.v4.min.js"
                    charset="utf-8"></script>
        </div>
    {% endif %}
{% endif %}